<script type="text/javascript">

$(document).ready(function(){

var frm,fcpu=50;
var rmu,ucpu=50;
var host='';
var utime='';

//alert(<%version%>);


function sockram(){

   socket.emit('res');
   

  socket.on('res',function(data){

 console.info(data);
    frm=data['frm'];
    rmu=data['rmu'];
    host=data['host'];
    utime=data['utime'];

fcpu=(data['cpu'].idle/(data['cpu'].total))*100;
ucpu=((data['cpu'].total - fcpu)/(data['cpu'].total))*100;


fcpu=fcpu.toFixed(0);
ucpu=ucpu.toFixed(0);


  
  });



  $('#host').text(host);
  $('#utime').text(utime);
}


  

setInterval(sockram,1000);


var chart2 = c3.generate({
  bindto: document.getElementById('ram'),
    data: {
        columns: [
            ['Free', 100],
            ['Used', 100],
        ],
        type : 'donut',
        onclick: function (d, i) { console.log("onclick", d, i); },
        onmouseover: function (d, i) { console.log("onmouseover", d, i); },
        onmouseout: function (d, i) { console.log("onmouseout", d, i); }
    },
    donut: {
        title: "Ram Usage"
    },
    size: { height: 280, width: 240}
    
});


var chart1 = c3.generate({
  bindto: document.getElementById('usage'),
    data: {
        columns: [
            ['Free', 100],
            ['Used', 100],
        ],
        type : 'donut',
        onclick: function (d, i) { console.log("onclick", d, i); },
        onmouseover: function (d, i) { console.log("onmouseover", d, i); },
        onmouseout: function (d, i) { console.log("onmouseout", d, i); }
    },
    donut: {
        title: "CPU Usage"
    },
    size: { height: 280, width: 240 }
});

setInterval(function(){
chart1.load({

columns: [
            ['Free', fcpu],
            ['Used', ucpu],
        ]

});


chart2.load({

columns: [
            ['Free', frm],
            ['Used', rmu],
        ]

});

},4000);

setTimeout(function () {
    chart2.unload({
        ids: 'Free'
    });
    chart2.unload({
        ids: 'Used'
    });

    chart1.unload({
        ids: 'Free'
    });
    chart1.unload({
        ids: 'Used'
    });

   
}, 2500);






var chart4 = c3.generate({
  bindto: document.getElementById('netio'),
    data: {
        columns: [
            ['data1', 30, 200, 100, 400, 150, 250],
            ['data2', 50, 20, 10, 40, 15, 25]
        ]
    },
    size: { height: 280, width: 360 }
});


});
  
  
</script>
 
